local switch = folder:AddSwitch("Fast Glitch (legends rock 1m+)", function(bool)
    if bool then
        _G.autoglitch = true

        local player = game.Players.LocalPlayer
        local workspace = game:GetService("Workspace")
        local runService = game:GetService("RunService")
        local muscleEvent = player:FindFirstChild("muscleEvent") -- Ensure event exists

        if not muscleEvent then
            warn("muscleEvent not found!")
            return
        end

        local function interactWithRock()
            local success, err = pcall(function()
                local rock = workspace.machinesFolder["Rock Of Legends"] and workspace.machinesFolder["Rock Of Legends"].Rock
                if rock then
                    firetouchinterest(rock, player.Character.LeftHand, 0)
                    wait(0.01)
                    firetouchinterest(rock, player.Character.LeftHand, 1)
                else
                    warn("Rock not found!")
                end
            end)
            if not success then
                warn("Error interacting with rock: ", err)
            end
        end

        local function performAutoPunch()
            local punchTool = player.Backpack:FindFirstChild("Punch")
            if punchTool then
                player.Character.Humanoid:EquipTool(punchTool)
                local rightPunchArgs = { [1] = "punch", [2] = "rightHand" }
                muscleEvent:FireServer(unpack(rightPunchArgs))

                local leftPunchArgs = { [1] = "punch", [2] = "leftHand" }
                muscleEvent:FireServer(unpack(leftPunchArgs))
            else
                warn("Punch tool not found!")
            end
        end

        repeat
            interactWithRock()
            performAutoPunch()
            runService.Heartbeat:Wait() -- Use Heartbeat to reduce potential performance issues
        until not _G.autoglitch
    else
        _G.autoglitch = false
    end
end)

local switch = folder:AddSwitch("Fast Glitch (King rock 5m+)", function(bool)
    if bool then
        _G.autoglitch = true

        local player = game.Players.LocalPlayer
