Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(128, 128, 128) -- Grey color
Frame.Position = UDim2.new(0.1, 0, 0.1, 0)
Frame.Size = UDim2.new(0, 200, 0, 150)
Frame.Visible = true
Frame.Active = true
Frame.Draggable = true -- Makes the frame draggable

AutoKillEvilKarmaBtn.Parent = Frame
AutoKillEvilKarmaBtn.BackgroundColor3 = Color3.fromRGB(105, 105, 105) -- Darker grey for button
AutoKillEvilKarmaBtn.Size = UDim2.new(0, 180, 0, 50)
AutoKillEvilKarmaBtn.Position = UDim2.new(0, 10, 0, 10)
AutoKillEvilKarmaBtn.Text = "Auto Kill Evil Karma [OFF]"
AutoKillEvilKarmaBtn.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black text font

AutoKillGoodKarmaBtn.Parent = Frame
AutoKillGoodKarmaBtn.BackgroundColor3 = Color3.fromRGB(105, 105, 105) -- Darker grey for button
AutoKillGoodKarmaBtn.Size = UDim2.new(0, 180, 0, 50)
AutoKillGoodKarmaBtn.Position = UDim2.new(0, 10, 0, 70)
AutoKillGoodKarmaBtn.Text = "Auto Kill Good Karma [OFF]"
AutoKillGoodKarmaBtn.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black text font

-- Hide/Show Arrow Button
HideButton.Parent = ScreenGui
HideButton.BackgroundColor3 = Color3.fromRGB(105, 105, 105) -- Darker grey for the button
HideButton.Size = UDim2.new(0, 50, 0, 50)
HideButton.Position = UDim2.new(0.1, -60, 0.1, 0) -- Initially placed outside of Frame
HideButton.Text = ">" -- Arrow to hide initially
HideButton.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black text

local isFrameVisible = true

HideButton.MouseButton1Click:Connect(function()
    isFrameVisible = not isFrameVisible
    Frame.Visible = isFrameVisible
    HideButton.Text = isFrameVisible and ">" or "<" -- Change the arrow direction
    HideButton.Position = isFrameVisible and UDim2.new(0.1, -60, 0.1, 0) or UDim2.new(0.1, 0, 0.1, 0)
end)

-- Killing Code
local function isValidKarma(player, validKarmaList)
    local karmaText = nil
    if player.Character and player.Character:FindFirstChild("Head") then
        if player.Character.Head:FindFirstChild("nameGui") and player.Character.Head.nameGui:FindFirstChild("karmaLabel") then
            karmaText = player.Character.Head.nameGui.karmaLabel.Text
        end
    end

    if karmaText then
        for _, valid in ipairs(validKarmaList) do
            if karmaText == valid then
                return true
            end
        end
    end
    return false
end

local function checkPlayer(player, validKarmaList)
    if player.Name ~= game.Players.LocalPlayer.Name and isValidKarma(player, validKarmaList) then
        local mycharacter = game.Players.LocalPlayer.Character
        if mycharacter and mycharacter:FindFirstChild("LeftHand") then
            local characterkiller = player.Character
            if characterkiller and characterkiller:FindFirstChild("Head") then
                characterkiller.Head.Anchored = true
                characterkiller.Head.CanCollide = false
                characterkiller.Head.Position = Vector3.new(
                    mycharacter.LeftHand.Position.X,
                    mycharacter.LeftHand.Position.Y,
                    mycharacter.LeftHand.Position.Z
                )

                local args = { [1] = "punch", [2] = "leftHand" }
                game:GetService("Players").LocalPlayer.muscleEvent:FireServer(unpack(args))

                local args2 = { [1] = "punch", [2] = "rightHand" }
                game:GetService("Players").LocalPlayer.muscleEvent:FireServer(unpack(args2))

                wait(0.1)
            end
        end
    end
end

local function autoKill(validKarmaList)
    _G.autokillplr = true
    for _, player in ipairs(game.Players:GetPlayers()) do
        checkPlayer(player, validKarmaList)
    end

    game.Players.PlayerAdded:Connect(function(newPlayer)
        newPlayer.CharacterAdded:Connect(function()
            checkPlayer(newPlayer, validKarmaList)
        end)
    end)

    repeat
        for _, player in ipairs(game.Players:GetPlayers()) do
            checkPlayer(player, validKarmaList)
        end
        wait(0.1)
    until not _G.autokillplr
end

-- Toggles for Auto Kill Evil Karma
local evilKarmaList = {
    "Peacekeeper", "Champion", "Avenger", "Hero", "Guardian", "Enforcer", "Protector", "Guard", "Nice"
}
local goodKarmaList = {
    "Gym Crusher", "Rookie Crusher", "Unleashed", "Enrage", "Stealth", "Hunter", "Evil", "Dark", "Tough"
}

AutoKillEvilKarmaBtn.MouseButton1Click:Connect(function()
    if _G.autokillplr then
        _G.autokillplr = false
        AutoKillEvilKarmaBtn.Text = "Auto Kill Evil Karma [OFF]"
    else
        _G.autokillplr = true
        AutoKillEvilKarmaBtn.Text = "Auto Kill Evil Karma [ON]"
        autoKill(evilKarmaList)
    end
end)

AutoKillGoodKarmaBtn.MouseButton1Click:Connect(function()
    if _G.autokillplr then
        _G.autokillplr = false
        AutoKillGoodKarmaBtn.Text = "Auto Kill Good Karma [OFF]"
    else
        _G.autokillplr = true
        AutoKillGoodKarmaBtn.Text = "Auto Kill Good Karma [ON]"
        autoKill(goodKarmaList)
    end
end)

end)
