folder:AddButton("AutoFarm weights (equip any farming tools)",function()
	while true do
		local args = {
			[1] = "rep"
		}

		game:GetService("Players").LocalPlayer.muscleEvent:FireServer(unpack(args))
		
		game:GetService("RunService").RenderStepped:Wait()
	end
end)


