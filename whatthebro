features:AddButton("Kill good/bad karma",function()
	local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local AutoKillEvilKarmaBtn = Instance.new("TextButton")
local AutoKillGoodKarmaBtn = Instance.new("TextButton")
local HideButton = Instance.new("TextButton") -- Arrow button to hide/show GUI
local UIS = game:GetService("UserInputService")

-- Parent the GUI to PlayerGui to survive death
